{
  "name": "dx-mgn-pool-react",
  "version": "0.0.1-RC11",
  "description": "Dx-Mgn-Pool React frontend",
  "main": "src/index.js",
  "scripts": {
    "build": "rm -rf dist && cross-env ETH_NETWORK=*net webpack --mode production",
    "build:dev": "rm -rf dist && cross-env SHOW_APP_DATA=true FE_CONDITIONAL_ENV='development' ETH_NETWORK=*net webpack --mode development",
    "build:dev:local": "rm -rf dist && SHOW_APP_DATA=true webpack --mode development",
    "build:local": "rm -rf dist && webpack --mode production",
    "dev": "webpack-dev-server --mode development --hot --content-base ./dist",
    "prod": "webpack-dev-server --mode production --hot --content-base ./dist",
    "lint": "eslint ./src ./migrations --ext .js,.jsx --ignore-path .gitignore --cache",
    "lint:fix": "npm run lint -- --fix",
    "migrate": "npx truffle migrate --reset",
    "start": "npm run dev",
    "start:local": "npm run dev",
    "start:localProd": "cross-env FE_CONDITIONAL_ENV='production' npm run dev",
    "start:shortTest": "cross-env SHORT_TEST='true' npm run dev",
    "rpc": "npx ganache-cli -d -e 10000000000",
    "deploy": "npm run setup-rpc",
    "setup-rpc": "npx truffle migrate",
    "fast-forward": "npx truffle exec trufflescripts/increase_time.js",
    "auction": "npx truffle exec trufflescripts/auction.js"
  },
  "author": "W3stside <doohhello@gmail.com>(https://github.com/w3stside)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gnosis/dx-mgn-pool-react.git"
  },
  "dependencies": {
    "@gnosis.pm/dx-mgn-pool": "^0.1.3-RC2",
    "create-subscription": "^16.8.2",
    "css-loader": "^0.28.11",
    "localforage": "^1.7.1",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-hot-loader": "^4.6.5",
    "style-loader": "^0.23.1",
    "truffle": "^5.0.3",
    "truffle-contract": "4.0.4",
    "typeface-major-mono-display": "0.0.71",
    "typeface-permanent-marker": "0.0.72",
    "web3": "1.0.0-beta.37",
    "web3-eth": "^1.0.0-beta.37",
    "web3-utils": "^1.0.0-beta.37"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.49",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.49",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.49",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "@babel/preset-flow": "^7.0.0-beta.49",
    "@babel/preset-react": "^7.0.0-beta.49",
    "@babel/register": "^7.0.0-beta.49",
    "@babel/runtime": "^7.0.0-beta.49",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^8.0.0-beta.2",
    "babel-plugin-import-redirect": "^1.1.1",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "cache-loader": "^1.2.2",
    "cross-env": "^5.1.6",
    "dotenv": "^6.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.10.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "file-loader": "^3.0.1",
    "glob": "^7.1.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "json-x-loader": "^0.3.0",
    "mini-css-extract-plugin": "^0.4.0",
    "minimist": "^1.2.0",
    "terser-webpack-plugin": "^1.2.3",
    "truffle-hdwallet-provider": "^0.0.6",
    "val-loader": "^1.1.0",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.3",
    "webpack-dev-server": "^3.1.4"
  }
}
